{
  "name": "My-Ext",
  "description": "插件描述",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [{
      "js": [
          "./pagenote-5.4.7/pagenote.js",
          "./libs/underscore-1.8.3.js",
          "./libs/md5-browser.js",
          "./libs/Readability.js",
          "./libs/html2md-1.0.js",
          "./libs/html2canvas.js",
          "./libs/qrcode.js",
          "./libs/lodash.js",
          "./tools.js",
          "./feynman.js"
      ],
      "css": [
          "./pagenote-5.4.7/pagenote.css",
          "./feynman.css"
      ],
      "matches": [
          "https://*/*",
          "http://*/*"
      ],
      "run_at": "document_end"
  }],
  "permissions": [
      "contextMenus",
      "notifications",
      "storage",
      "tabs",
      "activeTab",
      "scripting",
      "browsingData",
      "cookies",
      "<all_urls>"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "192": "logo.png"
  }
}